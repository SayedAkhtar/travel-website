{
  "kind": "collectionType",
  "collectionName": "activities",
  "info": {
    "singularName": "activity",
    "pluralName": "activities",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "video": {
      "type": "text"
    },
    "overview": {
      "type": "richtext"
    },
    "price": {
      "type": "integer",
      "default": 0
    },
    "discount_percent": {
      "type": "decimal",
      "default": 0
    },
    "activity_highlight": {
      "type": "richtext"
    },
    "min_people": {
      "type": "integer",
      "default": 0
    },
    "max_people": {
      "type": "integer",
      "default": 0
    },
    "start_time": {
      "type": "datetime"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "duration": {
      "type": "string"
    },
    "booking_period": {
      "type": "integer",
      "required": false
    },
    "activity_exclusion": {
      "type": "json"
    },
    "activity_inclusion": {
      "type": "json"
    },
    "highlights": {
      "type": "json"
    },
    "deposit": {
      "type": "enumeration",
      "enum": [
        "Disallow Deposit",
        "Deposit by Percent"
      ]
    },
    "deposit_value": {
      "type": "decimal"
    },
    "allow_cancel": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ]
    },
    "last_updated_by": {
      "type": "string"
    },
    "creator": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Activity",
        "Tour"
      ]
    },
    "category_type": {
      "type": "string"
    },
    "states": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::state.state",
      "inversedBy": "products"
    }
  }
}
