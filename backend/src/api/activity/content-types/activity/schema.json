    "activity_highlight": {
      "type": "richtext"
    },
    "activity_exclusion": {
      "type": "richtext"
    },
    "activity_inclusion": {
      "type": "richtext"
    },
    "min_people": {
      "type": "integer",
      "default": 0
    },
    "max_people": {
      "type": "integer",
      "default": 0
    },
    "start_time": {
      "type": "datetime"
    },
    "duration": {
      "type": "decimal",
      "default": 0
