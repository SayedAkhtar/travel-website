{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "video": {
      "type": "text"
    },
    "overview": {
      "type": "richtext"
    },
    "price": {
      "type": "integer",
      "default": 0
    },
    "discount_percent": {
      "type": "decimal",
      "default": 0
    },
    "highlight": {
      "type": "richtext"
    },
    "min_people": {
      "type": "integer",
      "default": 0
    },
    "max_people": {
      "type": "integer",
      "default": 0
    },
    "start_time": {
      "type": "time"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "duration": {
      "type": "string"
    },
    "booking_period": {
      "type": "integer",
      "required": false
    },
    "activity_exclusion": {
      "type": "json"
    },
    "activity_inclusion": {
      "type": "json"
    },
    "deposit": {
      "type": "enumeration",
      "enum": [
        "Disallow Deposit",
        "Deposit by Percent"
      ]
    },
    "deposit_value": {
      "type": "decimal"
    },
    "allow_cancel": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ]
    },
    "last_updated_by": {
      "type": "string"
    },
    "creator": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "category_type": {
      "type": "string"
    }
  }
}
