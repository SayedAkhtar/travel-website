{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "phone_number": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "fullname": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "password": {
      "type": "password"
    },
    "profile_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
